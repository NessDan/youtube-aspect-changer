{
  "manifest_version": 3,
  "name": "NessDan's YouTube Stretch-to-Fit Aspect Ratio Scaler",
  "version": "1.0",
  "description": "Scales a YouTube video up to remove horizontal black-bars.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}

{
  "manifest_version": 3,
  "name": "NessDan's YouTube Scale-to-Fit Vertically",
  "version": "1.1",
  "description": "Scales a YouTube video up to remove horizontal black-bars.",
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
